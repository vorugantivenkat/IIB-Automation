<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <xsd:element name="AchMandateAmendment">
    	<xsd:complexType>
    		<xsd:sequence>
    			<xsd:element name="Channel" minOccurs="1">
    				<xsd:simpleType>
    					<xsd:restriction base="xsd:string">
    						<xsd:pattern value="\w{0,10}"></xsd:pattern>
    						<xsd:pattern value="\w{1,10}"></xsd:pattern>
    					</xsd:restriction>
    				</xsd:simpleType>
    			</xsd:element>    			<xsd:element name="Reference" minOccurs="0">    				<xsd:simpleType>    					<xsd:restriction base="xsd:string">    						<xsd:pattern value="\w{1,34}"></xsd:pattern>    					</xsd:restriction>    				</xsd:simpleType>    			</xsd:element>    			<xsd:element name="Reason" minOccurs="1">    				<xsd:simpleType>    					<xsd:restriction base="xsd:string">    						<xsd:pattern value="\w{1,3}"></xsd:pattern>    					</xsd:restriction>    				</xsd:simpleType>    			</xsd:element>    			<xsd:element name="AdditionalInformation"    				minOccurs="0">    				<xsd:simpleType>    					<xsd:restriction base="xsd:string">    						<xsd:pattern value="\w{1,105}"></xsd:pattern>    					</xsd:restriction>    				</xsd:simpleType>    			</xsd:element>    			<xsd:element name="Mandate" minOccurs="1" maxOccurs="1">    				<xsd:complexType>    					<xsd:sequence>    						<xsd:element name="Reference"    							minOccurs="1">    							<xsd:simpleType>    								<xsd:restriction    									base="xsd:string">    									<xsd:pattern value="\w{1,20}"></xsd:pattern>    								</xsd:restriction>    							</xsd:simpleType>    						</xsd:element>    						<xsd:element name="Frequency"    							minOccurs="1">    							<xsd:simpleType>    								<xsd:restriction    									base="xsd:string">    									<xsd:pattern value="\w{1,4}"></xsd:pattern>    								</xsd:restriction>    							</xsd:simpleType>    						</xsd:element>    						<xsd:element name="RecurringFrequency"    							minOccurs="0">    							<xsd:simpleType>    								<xsd:restriction    									base="xsd:string">    									<xsd:pattern value="\d{1,2}"></xsd:pattern>    								</xsd:restriction>    							</xsd:simpleType>    						</xsd:element>    						<xsd:element name="PresentEach"    							minOccurs="0">    							<xsd:simpleType>    								<xsd:restriction    									base="xsd:string">    									<xsd:pattern value="\w{1,3}"></xsd:pattern>    								</xsd:restriction>    							</xsd:simpleType>    						</xsd:element>    						<xsd:element name="NumberOfPayments"    							minOccurs="0">    							<xsd:simpleType>    								<xsd:restriction    									base="xsd:string">    									<xsd:pattern value="\d{1,3}"></xsd:pattern>    								</xsd:restriction>    							</xsd:simpleType>    						</xsd:element>    						<xsd:element name="FromDate"    							minOccurs="1">    							<xsd:simpleType>    								<xsd:restriction    									base="xsd:string">    									<xsd:pattern value="\w{0,4}-\w{0,2}-\w{0,2}"></xsd:pattern>    								</xsd:restriction>    							</xsd:simpleType>    						</xsd:element>    						<xsd:element name="ToDate" minOccurs="1">    							<xsd:simpleType>    								<xsd:restriction    									base="xsd:string">    									<xsd:pattern value="\d{0,4}-\d{0,2}-\d{0,2}"></xsd:pattern>    								</xsd:restriction>    							</xsd:simpleType>    						</xsd:element>    						<xsd:choice minOccurs="1">    							<xsd:element name="CollectionAmount"    								minOccurs="0">    								<xsd:simpleType>    									<xsd:restriction    										base="xsd:string">    										<xsd:pattern    											value="\d{0,18}.\d{0,5}">    										</xsd:pattern>    									</xsd:restriction>    								</xsd:simpleType>    							</xsd:element>    							<xsd:element name="MaximumAmount"    								minOccurs="0">    								<xsd:simpleType>    									<xsd:restriction    										base="xsd:string">    										<xsd:pattern    											value="\d{0,18}.\d{0,5}">    										</xsd:pattern>    									</xsd:restriction>    								</xsd:simpleType>    							</xsd:element>    						</xsd:choice>    						<xsd:element name="Currency"    							minOccurs="0">    							<xsd:simpleType>    								<xsd:restriction    									base="xsd:string">    									<xsd:pattern value="\w{0,3}"></xsd:pattern>    								</xsd:restriction>    							</xsd:simpleType>    						</xsd:element>    						<xsd:element name="Reason" minOccurs="1">    							<xsd:simpleType>    								<xsd:restriction    									base="xsd:string">    									<xsd:pattern value="\w{1,4}"></xsd:pattern>    								</xsd:restriction>    							</xsd:simpleType>    						</xsd:element>    						<xsd:element name="Type" minOccurs="0">    							<xsd:simpleType>    								<xsd:restriction    									base="xsd:string">    									<xsd:pattern value="\w{0,11}"></xsd:pattern>    								</xsd:restriction>    							</xsd:simpleType>    						</xsd:element>    						<xsd:element name="AutoPostPayment"    							type="xsd:string" minOccurs="0">    						</xsd:element>    						<xsd:element name="Creditor" minOccurs="1">    							<xsd:complexType>    								<xsd:sequence>    									<xsd:element name="Bank"    										minOccurs="1">    										<xsd:simpleType>    											<xsd:restriction    												base="xsd:string">    												<xsd:pattern    													value="\w{0,35}">    												</xsd:pattern>    											</xsd:restriction>    										</xsd:simpleType>    									</xsd:element>    									<xsd:element name="Account"    										minOccurs="1">    										<xsd:simpleType>    											<xsd:restriction    												base="xsd:string">    												<xsd:pattern    													value="\w{1,34}">    												</xsd:pattern>    											</xsd:restriction>    										</xsd:simpleType>    									</xsd:element>    									<xsd:element name="Name"    										minOccurs="1">    										<xsd:simpleType>    											<xsd:restriction    												base="xsd:string">    												<xsd:pattern    													value="\w{1,140}">    												</xsd:pattern>    											</xsd:restriction>    										</xsd:simpleType>    									</xsd:element>    									<xsd:element name="Id"    										minOccurs="0">    										<xsd:simpleType>    											<xsd:restriction    												base="xsd:string">    												<xsd:pattern    													value="\w{0,35}">    												</xsd:pattern>    											</xsd:restriction>    										</xsd:simpleType>    									</xsd:element>    									<xsd:element name="IdType"    										minOccurs="0">    										<xsd:simpleType>    											<xsd:restriction    												base="xsd:string">    												<xsd:pattern    													value="\w{0,35}">    												</xsd:pattern>    											</xsd:restriction>    										</xsd:simpleType>    									</xsd:element>    								</xsd:sequence>    							</xsd:complexType>    						</xsd:element>    						<xsd:element name="Debtor" minOccurs="1">    							<xsd:complexType>    								<xsd:sequence>    									<xsd:element name="Bank"    										minOccurs="1">    										<xsd:simpleType>    											<xsd:restriction    												base="xsd:string">    												<xsd:pattern    													value="\w{0,35}">    												</xsd:pattern>    											</xsd:restriction>    										</xsd:simpleType>    									</xsd:element>    									<xsd:element name="Account"    										minOccurs="1">    										<xsd:simpleType>    											<xsd:restriction    												base="xsd:string">    												<xsd:pattern    													value="\w{0,34}">    												</xsd:pattern>    											</xsd:restriction>    										</xsd:simpleType>    									</xsd:element>    									<xsd:element name="Name"    										minOccurs="1">    										<xsd:simpleType>    											<xsd:restriction    												base="xsd:string">    												<xsd:pattern    													value="\w{0,140}">    												</xsd:pattern>    											</xsd:restriction>    										</xsd:simpleType>    									</xsd:element>    									<xsd:element name="Id"    										minOccurs="0">    										<xsd:simpleType>    											<xsd:restriction    												base="xsd:string">    												<xsd:pattern    													value="\w{0,35}">    												</xsd:pattern>    											</xsd:restriction>    										</xsd:simpleType>    									</xsd:element>    									<xsd:element name="IdType"    										minOccurs="0">    										<xsd:simpleType>    											<xsd:restriction    												base="xsd:string">    												<xsd:pattern    													value="\w{0,35}">    												</xsd:pattern>    											</xsd:restriction>    										</xsd:simpleType>    									</xsd:element>    								</xsd:sequence>    							</xsd:complexType>    						</xsd:element>    					</xsd:sequence>    				</xsd:complexType>    			</xsd:element>    		</xsd:sequence>    	</xsd:complexType>    </xsd:element></xsd:schema>