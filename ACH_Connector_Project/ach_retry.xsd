<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:element name="AchDirectCreditRecovery">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Channel" minOccurs="1" maxOccurs="1">					<xsd:simpleType>						<xsd:restriction base="xsd:string">							<xsd:pattern value=".{0,10}"></xsd:pattern>						</xsd:restriction>					</xsd:simpleType>				</xsd:element>				<xsd:element name="BatchReference" minOccurs="1">					<xsd:simpleType>						<xsd:restriction base="xsd:string">							<xsd:pattern value=".{0,16}"></xsd:pattern>						</xsd:restriction>					</xsd:simpleType>				</xsd:element>				<xsd:element name="Transaction" minOccurs="0">					<xsd:complexType>						<xsd:sequence>							<xsd:element name="TransactionReference" minOccurs="1">								<xsd:simpleType>									<xsd:restriction base="xsd:string">										<xsd:pattern value=".{0,16}"></xsd:pattern>									</xsd:restriction>								</xsd:simpleType>							</xsd:element>							<xsd:element name="UserReference" minOccurs="0">								<xsd:simpleType>									<xsd:restriction base="xsd:string">										<xsd:pattern value=".{0,35}"></xsd:pattern>									</xsd:restriction>								</xsd:simpleType>							</xsd:element>							<xsd:element name="BeneficiaryBank" minOccurs="1">								<xsd:simpleType>									<xsd:restriction base="xsd:string">										<xsd:pattern value=".{0,35}"></xsd:pattern>									</xsd:restriction>								</xsd:simpleType>							</xsd:element>							<xsd:element name="BeneficiaryAccount" minOccurs="1">								<xsd:simpleType>									<xsd:restriction base="xsd:string">										<xsd:pattern value=".{0,34}"></xsd:pattern>									</xsd:restriction>								</xsd:simpleType>							</xsd:element>							<xsd:element name="BeneficiaryName" minOccurs="0">								<xsd:simpleType>									<xsd:restriction base="xsd:string">										<xsd:pattern value=".{0,140}"></xsd:pattern>									</xsd:restriction>								</xsd:simpleType>							</xsd:element>							<xsd:element name="BeneficiaryId" minOccurs="0">								<xsd:simpleType>									<xsd:restriction base="xsd:string">										<xsd:pattern value=".{0,35}"></xsd:pattern>									</xsd:restriction>								</xsd:simpleType>							</xsd:element>							<xsd:element name="BeneficiaryIdType" minOccurs="0">								<xsd:simpleType>									<xsd:restriction base="xsd:string">										<xsd:pattern value=".{0,35}"></xsd:pattern>									</xsd:restriction>								</xsd:simpleType>							</xsd:element>							<xsd:element name="PurposeOfTransfer" minOccurs="0">								<xsd:simpleType>									<xsd:restriction base="xsd:string">										<xsd:pattern value=".{0,4}"></xsd:pattern>									</xsd:restriction>								</xsd:simpleType>							</xsd:element>							<xsd:element name="DetailsOfPayments" minOccurs="0">								<xsd:simpleType>									<xsd:restriction base="xsd:string">										<xsd:pattern value=".{0,140}"></xsd:pattern>									</xsd:restriction>								</xsd:simpleType>							</xsd:element>						</xsd:sequence>					</xsd:complexType>				</xsd:element>			</xsd:sequence>			<xsd:attribute name="OptionIdentifier" type="xsd:string"></xsd:attribute>		</xsd:complexType>	</xsd:element></xsd:schema>